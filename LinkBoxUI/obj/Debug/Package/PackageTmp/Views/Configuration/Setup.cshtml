<div class="modal fade" id="create-modal-addon">
    <div class="modal-dialog">
        <form id="addon-submit">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                    <h4 class="modal-title">New Addon Setup</h4>
                </div>
                <div class="modal-body">
                    <div class="box-body">
                        <div class="form-group" id="create-addon-code-formgroup">
                            <label>*Addon Code:</label>
                            <div class="input-group">
                                <div class="input-group-addon">
                                    <i class="fa fa-database"></i>
                                </div>
                                <input type="text" class="form-control addon" placeholder="Addon Code" id="addon-code" required>
                            </div>
                        </div>
                        <div class="form-group" id="create-addon-dbversion-formgroup">
                            <label>*DB Version:</label>
                            <div class="input-group">
                                <div class="input-group-addon">
                                    <i class="fa fa-database"></i>
                                </div>
                                <select class="form-control select2" style="width: 100%;" id="addon-dbversion" required>
                                    @foreach (var item in Model.SAPList)
                                    {
                                        <option>@item.SAPDBVersion</option>
                                    }
                                </select>
                            </div>
                        </div>
                        <div class="form-group" id="create-addon-server-formgroup">
                            <label>*Server Name:</label>
                            <div class="input-group">
                                <div class="input-group-addon">
                                    <i class="fa fa-server"></i>
                                </div>
                                <input type="text" class="form-control" placeholder="Server Name" id="addon-server" required>
                            </div>
                        </div>
                        <div class="form-group" id="create-addon-ip-formgroup">
                            <label>*IP Address:</label>
                            <div class="input-group">
                                <div class="input-group-addon">
                                    <i class="fa fa-laptop"></i>
                                </div>
                                <input type="text" class="form-control" data-inputmask="'alias': 'ip'" data-mask id="addon-ip" required>
                            </div>
                        </div>

                        <div class="form-group" id="create-addon-dbname-formgroup">
                            <label>*Database Name:</label>
                            <div class="input-group">
                                <div class="input-group-addon">
                                    <i class="fa fa-server"></i>
                                </div>
                                <input type="text" class="form-control" placeholder="Database Name" id="addon-dbname" required>
                            </div>
                        </div>

                        <div class="form-group">
                            <label>Port:</label>
                            <div class="input-group">
                                <div class="input-group-addon">
                                    <i class="fa fa-wrench"></i>
                                </div>
                                <input type="number" class="form-control" placeholder="Port" id="addon-port">
                            </div>
                        </div>

                        <div class="form-group" id="create-addon-userid-formgroup">
                            <label>*DB User ID:</label>
                            <div class="input-group">
                                <div class="input-group-addon">
                                    <i class="fa fa-user"></i>
                                </div>
                                <input type="text" class="form-control" placeholder="DB User ID" id="addon-userid" required>
                            </div>
                        </div>

                        <div class="form-group" id="create-addon-password-formgroup">
                            <label>*DB Password:</label>
                            <div class="input-group" id="password">
                                <div class="input-group-addon">
                                    <i class="fa fa-lock"></i>
                                </div>
                                <input type="password" class="form-control" placeholder="DB Password" id="addon-password" required>
                                <div class="input-group-addon" id="passeye">
                                    <a href="" id="passeye1"><i class="fa fa-eye" id="eye1"></i></a>
                                </div>
                            </div>
                        </div>

                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default btn-custom " data-dismiss="modal">Close</button>
                    <button type="submit" class="btn btn-primary btn-custom " id="add-addon-setup">Save</button>
                </div>
            </div>

        </form>

        <!-- /.modal-content -->
    </div>
    <!-- /.modal-dialog -->
</div>

