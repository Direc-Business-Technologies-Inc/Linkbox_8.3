                    <div class="tab-pane active" id="tab_1">
                        <div class="box">
                            <div class="box-header">
                                <h3 class="box-title"><i class="fa fa-database"></i> Path Setup</h3>
                            </div>
                            <div class="box-body">
                                <table id="PathConfig" class="table table-bordered table-striped dt">
                                    <thead>
                                        <tr>
                                            <th>Edit</th>
                                            <th>Code</th>
                                            <th>Local Path</th>
                                            <th>Backup Path</th>
                                            <th>Error Path</th>

                                            <th></th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @foreach (var path in Model.PathView)
                                        {

                                            <tr>
                                                <td width="5%"><button type="button" class="btn-info btn-flat fa fa-edit" data-toggle="modal" data-target="#edit-modal-path" data-path-id="@path.PathId"></button></td>
                                                <td>@path.PathCode</td>
                                                <td>@path.LocalPath</td>
                                                <td>@path.BackupPath</td>
                                                <td>@path.ErrorPath</td>

                                                @if (path.IsActive == true)
                                                {
                                                    <td><span class="label label-success">Active</span></td>
                                                }
                                                else
                                                {
                                                    <td><span class="label label-danger">In Active</span></td>
                                                }
                                            </tr>

                                        }
                                    </tbody>
                                </table>
                                <button type="button" class="btn btn-primary btn-custom2 " style="float: right;" data-toggle="modal" data-target="#create-modal-path">New</button>
                            </div>
                        </div>
                    </div>

